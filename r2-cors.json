{
  "rules": [
    {
      "allowed": {
        "origins": [
          "https://v.bilgekarga.tr",
          "https://cdn.bilgekarga.tr",
          "http://localhost:8787",
          "http://localhost:5173",
          "http://127.0.0.1:8787",
          "http://127.0.0.1:5173"
